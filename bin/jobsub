#!/usr/bin/env ruby
#
# Test out submitting a job
#

require "rsgejob"

script = <<EOF
#!/bin/bash
#

hostname
EOF

job = RsgeJob.new( {
    :job_name   => "test_job",
    :cwd        => true,
    :script     => script,
    :hard_resreq => ["mem_free=1G", "pcpus=1"]
} )

job.submit

puts job.jobid + " " + job.job_owner
